{
	"name": "@laptopoutlet/admin",
	"private": true,
	"version": "1.0.0",
	"scripts": {
		"codegen": "graphql-let",
		"dev": "graphql-let && next dev -p 8000",
		"build": "graphql-let && next build",
		"typecheck": "graphql-let && tsc -p ./ --noEmit",
		"clean": "rimraf --no-glob ./.next ./out ./cache",
		"start": "next start -p 8000",
		"deps:check": "npx npm-check-updates --dep prod,dev,optional",
		"deps:update": "npx npm-check-updates --dep prod,dev,optional -u"
	},
	"dependencies": {
