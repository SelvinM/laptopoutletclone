		"@laptopoutlet-packages/constants": "*",
		"@laptopoutlet-packages/models": "*",
		"@laptopoutlet-packages/utils": "*",
		"@laptopoutlet-packages/placeholder": "*",
		"@sendgrid/mail": "^7.6.0",
		"antd": "^4.17.2",
		"apollo-server-micro": "^2.25.2",
		"apollo-upload-client": "^16.0.0",
		"argon2": "^0.28.3",
		"cookie": "^0.4.1",
		"dayjs": "^1.10.7",
		"deepmerge": "^4.2.2",
